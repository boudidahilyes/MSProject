    email: String
    metadata: MapScalar
}

type FailureEventData {
    eventId: ID!
    String: String
    timestamp: String
    email: String
    metadata: MapScalar
}


type Query {
    getSuccessEvents: [SuccessEventData]
    getFailureEvents: [FailureEventData]
}